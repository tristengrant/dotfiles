# Git aliases
alias ga="git add ."
alias gp="git push"
alias gc="git commit -m"
alias gs="git status -sb"
alias gac="git add . && git commit -m"
alias pushit="git push"

# General
alias ls='ls --color=auto'
alias ll='ls -alFh'
alias c='clear'

# Directory
alias ..='cd ..'
alias ...='cd ../..'
alias ....='cd ../../..'
alias tgsite='cd ~/Github/tristengrant/'
alias cbsite='cd ~/Github/catandbotcomic/'

# File management
alias md='mkdir -p'
alias rm='rm -i'
alias cp='cp -i'
alias mv='mv -i'
alias hg='history | grep'
